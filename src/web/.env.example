# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_VERSION=v1
VITE_API_KEY_HEADER=X-API-Key

# WebSocket Configuration
VITE_WS_BASE_URL=ws://localhost:8000
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RETRIES=5
VITE_WS_PING_INTERVAL=30000

# Timeout Settings (in milliseconds)
VITE_GENERATION_TIMEOUT=100000
VITE_CONTROL_TIMEOUT=5000
VITE_STATUS_TIMEOUT=3000

# Retry Configuration
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_INTERVAL=60000

# Validation Rules
VITE_MAX_PROMPT_LENGTH=1000
VITE_MAX_FRAME_COUNT=102

# Monitoring and Telemetry
VITE_TELEMETRY_ENABLED=true
VITE_ERROR_REPORTING_URL=http://localhost:8001
VITE_LOG_LEVEL=info
VITE_METRICS_ENABLED=true